{
  "name": "cabildo-abierto",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "pnpm --filter cabildo-abierto dev",
    "dev:backend": "pnpm --filter ca-server dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rimraf node_modules"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}