Indexes:
    "Content_pkey" PRIMARY KEY, btree (uri)
    "Content_created_at_idx" btree (created_at DESC)
    "Content_created_at_tz_idx" btree (created_at_tz DESC)
    "Content_interactionsScore_created_at_idx" btree ("interactionsScore" DESC, created_at DESC)
    "Content_likesScore_created_at_idx" btree ("likesScore" DESC, created_at DESC)
    "Content_relativePopularityScore_created_at_idx" btree ("relativePopularityScore" DESC, created_at DESC)
    "content_text_fts_idx" gin (text_tsv)
Foreign-key constraints:
    "Content_textBlobId_fkey" FOREIGN KEY ("textBlobId") REFERENCES "Blob"(cid) ON UPDATE CASCADE ON DELETE SET NULL
    "Content_uri_fkey" FOREIGN KEY (uri) REFERENCES "Record"(uri) ON UPDATE CASCADE ON DELETE RESTRICT
