{
  "lexicon": 1,
  "id": "ar.cabildoabierto.embed.poll",
  "defs": {
    "main": {
      "type": "object",
      "required": ["id","choices"],
      "properties": {
        "id" : {
          "type":  "string"
        },
        "description": {
          "type": "string"
        },
        "choices": {
          "type": "array",
          "items": {
            "type": "ref",
            "ref": "#pollChoice"
          }
        }
      }
    },
    "pollChoice": {
      "type": "object",
      "required": ["label","votes"],
      "properties": {
        "label": {"type": "string"},
        "votes": {"type": "integer"}
      }
    }
  }

}